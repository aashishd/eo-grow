{
  "pipeline": "eogrow.pipelines.rasterize.RasterizePipeline",
  "**global_config": "${config_path}/global_config.json",
  "input_folder_key": "reference",  // This one rasterizes from a vector feature, that is added in tests 
  "output_folder_key": "reference",
  "resolution": 10,
  "vector_input": ["vector_timeless", "LULC_VECTOR"],
  "columns": [
    {
      "column_name": "LULC_ID",
      "output_feature": "LULC_ID",
      "polygon_buffer": 0
    },
    {
      "column_name": "LULC_POLYGON_ID",
      "output_feature": "LULC_POLYGON_ID",
      "polygon_buffer": 0
    },
    {
      "column_name": "LULC_POLYGON_ID",
      "output_feature": "LULC_POLYGON_ERODED_01",
      "polygon_buffer": -10
    },
    {
      "column_name": "LULC_POLYGON_ID",
      "output_feature": "LULC_POLYGON_ERODED_02",
      "polygon_buffer": -15
    }
  ],
  "overlap_value": 0,
  "dtype": "int16",
  "no_data_value": 0,
  "workers": 2
}